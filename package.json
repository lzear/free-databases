{
  "name": "todos",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pscale connect todos dev --port 3309 & next dev",
    "pscale-db-push": "prisma db push",
    "pscale-connect": "pscale connect todos dev --port 3309",
    "drizzle-generate": "drizzle-kit generate:pg --schema=./src/data-providers/drizzle/index.ts",
    "drizzle-migrate": "node --loader ts-node/esm --es-module-specifier-resolution=node --experimental-vm-modules --no-warnings ./src/bin/flyio/migrate.ts",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "cypress": "cypress open",
    "e2e": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\""
  },
  "dependencies": {
    "@prisma/client": "^4.14.0",
    "@supabase/supabase-js": "^2.21.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/lodash": "^4.14.195",
    "@types/node": "20.1.3",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "cypress": "12.3.0",
    "date-fns": "^2.30.0",
    "dotenv-cli": "^7.2.1",
    "drizzle-kit": "^0.18.1",
    "drizzle-orm": "^0.26.1",
    "encoding": "^0.1.13",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.2",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "hsluv": "^1.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.2",
    "next": "13.4.2",
    "postgres": "^3.3.4",
    "prettier": "^2.8.8",
    "primeicons": "^6.0.1",
    "primereact": "^9.4.0",
    "prisma": "^4.14.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "^0.0.1",
    "start-server-and-test": "1.15.2",
    "typescript": "5.0.4"
  }
}
