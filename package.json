{
  "name": "free-databases",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "resolutions": {
    "better-sqlite3": "^7.6.2"
  },
  "scripts": {
    "dev": "yarn pscale-connect & next dev",
    "pscale-db-push": "prisma db push",
    "pscale-connect": "pscale connect todos dev --port 3309",
    "env": "dotenv -e .env",
    "drizzle-generate-postgres": "drizzle-kit generate:pg --schema=./src/drizzle-schema/postgres/index.ts",
    "drizzle-generate-mysql": "drizzle-kit generate:mysql --schema=./src/drizzle-schema/mysql/index.ts",
    "drizzle-generate-sqlite": "drizzle-kit generate:sqlite --schema=./src/drizzle-schema/sqlite/index.ts",
    "drizzle-migrate-flyio": "node --loader ts-node/esm --es-module-specifier-resolution=node --experimental-vm-modules --no-warnings ./src/bin/flyio/migrate.ts",
    "drizzle-migrate-tidbcloud": "node --loader ts-node/esm --es-module-specifier-resolution=node --experimental-vm-modules --no-warnings ./src/bin/tidbcloud/migrate.ts",
    "drizzle-migrate-yugabyte": "node --loader ts-node/esm --es-module-specifier-resolution=node --experimental-vm-modules --no-warnings ./src/bin/yugabyte/migrate.ts",
    "drizzle-migrate-railway": "node --loader ts-node/esm --es-module-specifier-resolution=node --experimental-vm-modules --no-warnings ./src/bin/railway/migrate.ts",
    "drizzle-migrate-aiven": "node --loader ts-node/esm --es-module-specifier-resolution=node --experimental-vm-modules --no-warnings ./src/bin/aiven/migrate.ts",
    "drizzle-migrate-cockroach": "node --loader ts-node/esm --es-module-specifier-resolution=node --experimental-vm-modules --no-warnings ./src/bin/cockroach/migrate.ts",
    "drizzle-migrate-elephant": "node --loader ts-node/esm --es-module-specifier-resolution=node --experimental-vm-modules --no-warnings ./src/bin/elephant/migrate.ts",
    "drizzle-migrate-neon": "node --loader ts-node/esm --es-module-specifier-resolution=node --experimental-vm-modules --no-warnings ./src/bin/neon/migrate.ts",
    "drizzle-migrate-vercel": "dotenv -e .env node --loader ts-node/esm --es-module-specifier-resolution=node --experimental-vm-modules --no-warnings ./src/bin/vercel/migrate.ts",
    "drizzle-migrate-turso": "node --loader ts-node/esm --es-module-specifier-resolution=node --experimental-vm-modules --no-warnings ./src/bin/turso/migrate.ts",
    "build": "prisma generate && next build && npx convex deploy",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "cypress": "cypress open",
    "e2e": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
    "ncu": "npx npm-check-updates --upgrade --root",
    "depcheck": "lerna exec --no-bail --stream -- depcheck"
  },
  "dependencies": {
    "@azure/cosmos": "^3.17.3",
    "@libsql/client": "^0.2.2",
    "@prisma/client": "^4.16.2",
    "@supabase/supabase-js": "^2.26.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.2",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@upstash/redis": "^1.22.0",
    "@vercel/postgres": "^0.4.0",
    "@xata.io/cli": "^0.12.6",
    "@xata.io/client": "^0.24.3",
    "convex": "^0.14.1",
    "cypress": "12.16.0",
    "date-fns": "^2.30.0",
    "deta": "^1.1.0",
    "dotenv-cli": "^7.2.1",
    "drizzle-kit": "^0.19.2",
    "drizzle-orm": "^0.27.0",
    "encoding": "^0.1.13",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "faunadb": "^4.8.0",
    "firebase": "^9.23.0",
    "firebase-admin": "^11.9.0",
    "hsluv": "^1.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lodash": "^4.17.21",
    "mysql2": "^3.4.3",
    "nanoid": "^4.0.2",
    "next": "13.4.7",
    "next-plausible": "^3.8.0",
    "node-gyp": "^9.4.0",
    "node-postgres": "^0.6.2",
    "pg": "^8.11.1",
    "postgres": "^3.3.5",
    "prettier": "^2.8.8",
    "primeicons": "^6.0.1",
    "primereact": "^9.6.0",
    "prisma": "^4.16.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "^0.0.1",
    "start-server-and-test": "2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "5.1.6",
    "vercel": "^31.0.1"
  },
  "volta": {
    "node": "18.16.0"
  }
}
